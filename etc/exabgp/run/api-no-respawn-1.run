#!/usr/bin/env python3

from exabgp_api import send, wait_for_ack


def main() -> None:
    messages = [
        'announce route 1.1.1.1/32 next-hop 11.11.11.11',
    ]

    message = messages.pop(0)
    send(message)
    wait_for_ack()
    # Exits immediately after ACK - no wait_for_shutdown


if __name__ == '__main__':
    main()
